{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"template/TemplateName/TemplateName.tsx",
        "style":"template/TemplateName/TemplateName.module.css",
        "story":"template/TemplateName/TemplateName.stories.tsx",
        "lazy":"template/TemplateName/TemplateName.lazy.tsx"
      }
    },
    "layout": {
      "path": "src/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"template/TemplateName/TemplateName.tsx",
        "style":"template/TemplateName/TemplateName.module.css",
        "story":"template/TemplateName/TemplateName.stories.tsx",
        "lazy":"template/TemplateName/TemplateName.lazy.tsx"
      }
    },
    "ui": {
      "path": "src/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"template/TemplateName/TemplateName.tsx",
        "style":"template/TemplateName/TemplateName.module.css",
        "story":"template/TemplateName/TemplateName.stories.tsx",
        "lazy":"template/TemplateName/TemplateName.lazy.tsx"
      }
    }
  }
}